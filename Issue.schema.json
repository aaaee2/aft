{
    "title": "Issue",
    "type": "object",
    "required": [
        "timestamp",
        "advertiserId",
        "webmaster",
        "type",
        "geo",
        "provider",
        "device",
        "route",
        "reportUrl"
    ],
    "properties": {
        "timestamp": {
            "type": "number",
            "minimum": 0
        },
        "advertiserId": {
            "type": "string"
        },
        "type": {
            "type": "IssueType"
        },
        "webmaster": {
            "type": "object",
            "required": [
                "source",
                "value"
            ],
            "properties": {
                "source": {
                    "type": "string",
                    "enum": ["id", "hash"]
                },
                "value": {
                    "type": "string"
                }
            }
        },
        "geo": {
            "type": "object",
            "required": [
                "country",
                "city"
            ],
            "properties": {
                "continent": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                }
            }
        },
        "provider": {
            "type": "IssueSourceProvider"
        },
        "device": {
            "type": "IssueSourceDevice"
        },
        "route": {
            "type": "array",
            "minItems": 2,
            "items": {
                "type": "object",
                "required": [
                    "code",
                    "url"
                ],
                "properties": {
                    "code": {
                        "type": "number",
                        "minimum": 200
                    },
                    "url": {
                        "type": "string"
                    }
                }
            }
        },
        "reportUrl": {
            "type": "string"
        },
        "_meta": {
            "type": "object",
            "properties": {
                "provider": {
                    "type": "string"
                },
                "device": {
                    "type": "string"
                }
            }
        }
    }
}
